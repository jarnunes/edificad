    "/distribuicao-cesta": {
      "get": {
        "tags": [
          "Distribuição Cesta"
        ],
        "summary": "Retorna um array com todas as distribuições registradas",
        "parameters": [
          {
            "name": "cesta",
            "in": "query",
            "description": "Nome da cesta",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "cpfBeneficiario",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "cpfVoluntario",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "data",
            "in": "query",
            "description": "Data de distribuição da cesta",
            "schema": {
              "type": "string",
              "example": "2023-10-28"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Sucesso",
            "content": {
